# Google Forms API Configuration
# Copy to .env.local and fill in your credentials

# Google Forms API Credentials
# Get these from Google Cloud Console
REACT_APP_GOOGLE_CLIENT_ID=your_client_id.apps.googleusercontent.com
REACT_APP_GOOGLE_CLIENT_SECRET=your_client_secret

# For Firebase Cloud Functions
GOOGLE_CLIENT_EMAIL=your_service_account@your_project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour_Private_Key_Here\n-----END PRIVATE KEY-----\n"
GOOGLE_CLIENT_ID=your_client_id

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Cloud Functions Base URL (deployed)
REACT_APP_FUNCTIONS_BASE_URL=https://us-central1-your_project.cloudfunctions.net

# Form Configuration
REACT_APP_FORM_RESPONSE_TIMEOUT=30000
REACT_APP_MAX_FORM_ATTACHMENT_SIZE=5242880
